<h1>Listing internal_categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @internal_categories.each do |internal_category| %>
      <tr>
        <td><%= internal_category.name %></td>
        <td><%= link_to 'Show', internal_category %></td>
        <td><%= link_to 'Edit', edit_internal_category_path(internal_category) %></td>
        <td><%= link_to 'Destroy', internal_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Internal category', new_internal_category_path %>
